pipeline {
    agent {
        label `node1`
    }
    tools {
        maven "maven3.6.3"
    }

    stages {
        stage('parallelstage') {
            parallel {
                stage ('checkoutcode'){
                    steps {
                git branch: 'development', credentialsId: '8707530d-7105-4261-8e79-46d0972584d0', url: 'https://github.com/ummar-DevOps/maven-web.git'
            }
                }
                stage ('hello'){
            steps {
                echo "hello world"
            }
        }
            
            }
        }
        stage ("maven bulid"){
            steps {
                sh 'mvn clean package'
            }
            }
        
    }//stages
}//pipeline
